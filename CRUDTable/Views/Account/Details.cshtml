@model Microsoft.AspNetCore.Identity.IdentityUser

@{
    ViewData["Title"] = "Xem chi tiết";
}

<h4 class="text-info">Chi tiết tài khoản</h4>

<div>
    <dl>
        <dt>Email</dt>
        <dd>@Model.Email</dd>
        <dt>Username</dt>
        <dd>@Model.UserName</dd>
        <!-- Add other user properties you want to display -->
    </dl>
</div>
